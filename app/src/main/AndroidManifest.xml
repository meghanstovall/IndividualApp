<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodjournal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".FifthScreen"
            android:parentActivityName=".FourthScreen">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FourthScreen" />
        </activity>
        <activity
            android:name=".FourthScreen"
            android:parentActivityName=".ThirdScreen">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ThirdScreen" />
        </activity>
        <activity
            android:name=".ThirdScreen"
            android:parentActivityName=".SecondScreen">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".SecondScreen" />
        </activity>
        <activity
            android:name=".SecondScreen"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>